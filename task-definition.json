{
  "family": "fastapi-greetings-task",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "arn:aws:iam::687238288658:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "fastapi-greetings",
      "image": "687238288658.dkr.ecr.eu-west-3.amazonaws.com/test/poc:fastapi-greetings-v1.0",
      "portMappings": [
        {
          "containerPort": 8000,
          "protocol": "tcp"
        }
      ],
      "essential": true,
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/fastapi-greetings",
          "awslogs-region": "eu-west-3",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ]
}